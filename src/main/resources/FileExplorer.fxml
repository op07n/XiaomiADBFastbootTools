<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="480.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FileExplorerController">
  <children>
      <AnchorPane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0">
         <children>
            <TextField fx:id="pathTextField" prefHeight="30.0" prefWidth="640.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="60.0" AnchorPane.topAnchor="10.0" />
            <Button fx:id="backButton" mnemonicParsing="false" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
         </children>
      </AnchorPane>
    <AnchorPane prefWidth="-1.0" VBox.vgrow="SOMETIMES">
      <children>
            <ListView fx:id="listView" layoutX="226.0" layoutY="98.0" onMouseClicked="#listViewMouseClicked" prefWidth="640.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
      </children>
    </AnchorPane>
      <AnchorPane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="30.0">
         <children>
            <TextField fx:id="statusTextField" editable="false" prefHeight="30.0" prefWidth="640.0" AnchorPane.bottomAnchor="9.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="201.0" AnchorPane.topAnchor="-1.0">
               <font>
                  <Font size="12.0" />
               </font>
            </TextField>
            <ProgressBar fx:id="progressBar" layoutX="22.0" layoutY="4.0" prefHeight="30.0" prefWidth="640.0" progress="0.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="452.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <AnchorPane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="40.0" VBox.vgrow="ALWAYS">
         <children>
            <FlowPane hgap="13.0" layoutX="132.0" layoutY="-24.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Button fx:id="pushButton" mnemonicParsing="false" onAction="#pushButtonPressed" text="Push to device">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="pullButton" mnemonicParsing="false" onAction="#pullButtonPressed" text="Pull from device">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="newFolderButton" mnemonicParsing="false" onAction="#newFolderButtonPressed" text="New Folder">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="renameButton" mnemonicParsing="false" onAction="#renameButtonPressed" text="Rename">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteButtonPressed" text="Delete">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </FlowPane>
         </children>
      </AnchorPane>
  </children>
</VBox>
